              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 

e.IdAction,
e.IdTypeEtape,
e.IdEtape,
LTRIM(RTRIM(t.TypeEtape)) as TypeEtape,
LTRIM(RTRIM(isnull(se.NomFichier,''))) as Fichier,
LTRIM(RTRIM(isnull(se.ExtensionFichier, ''))) as Extension,
LTRIM(RTRIM(isnull(e.DossierSource, ''))) as DossierSource,
LTRIM(RTRIM(isnull(e.DossierCible,''))) as DossierCible,
LTRIM(RTRIM(isnull(e.ChaineAChercher,''))) as ChaineAChercher,
LTRIM(RTRIM(isnull(m.[From],''))) as [From],
LTRIM(RTRIM(isnull(m.[To],''))) as [To],
LTRIM(RTRIM(isnull(m.Cc,''))) as [Cc],
LTRIM(RTRIM(isnull(m.Bcc,''))) as [Bcc],
LTRIM(RTRIM(isnull(m.Sujet,''))) as Sujet,
LTRIM(RTRIM(isnull(m.Corps,''))) as Corps,
LTRIM(RTRIM(isnull(f.ADRESSE,''))) as ADRESSE,
isnull(CAST(f.PORT AS INT),'') as PORT,
LTRIM(RTRIM(isnull(f.[LOGIN],''))) as [LOGIN],
LTRIM(RTRIM(isnull(f.PWD,''))) as PWD,
isnull(r.Position,'') as Postion,
isnull(r.Longueur,'') as Longueur,
LTRIM(RTRIM(isnull(r.ChaineAChercher,''))) as RenommerChaineAChercher,
LTRIM(RTRIM(isnull(r.ChaineRemplacement,''))) as RenommerChaineRemplacement,
LTRIM(RTRIM(isnull(s.[SERVER],''))) as [SERVER],
LTRIM(RTRIM(isnull(s.[SmtpLogin],''))) as [SmtpLogin],
LTRIM(RTRIM(isnull(s.[SmtpPwd],''))) as [SmtpPwd],
LTRIM(RTRIM(isnull(ps.NomInstance,''))) as [NomInstance],
LTRIM(RTRIM(isnull(ps.NomBDD,''))) as [NomBDD],
LTRIM(RTRIM(isnull(ps.NomProcedureStockee,''))) as [NomProcedureStockee],
LTRIM(RTRIM(isnull(ps.NomColonneRetour,''))) as [NomColonneRetour],
LTRIM(RTRIM(isnull(ps.ChaineDeParametres,''))) as [ChaineDeParametres],
isnull(e.AccuseReception,0) as AccuseReception,
CASE WHEN m.IdMail IS NOT NULL THEN 'smtp' ELSE 'ftp' END as ModeReceptionFichiers,
isnull(s.smtpTimeout,0) as smtpTimeout,
isnull(f.ftpTimeOut,0) as ftpTimeout

from Etape e
inner join TmpSousEtape se
	on e.IdAction = se.idAction
inner join TypeEtape t
	on e.IdTypeEtape = t.IdTypeEtape 
left join [dbo].[Email] m
	on e.IdMail = m.IdMail 
	and m.Actif = 1
left join [dbo].[FTP] f
	on e.idFtp = f.IdFTP
	and f.ACTIF = 1
left join [dbo].[Renommer] r
	on e.idRenommer = r.IdRenommer
	and r.Actif = 1
left join [dbo].[Smtp] s
	on e.idSmtp  = s.IdSMTP 
	and s.Actif = 1
left join [dbo].[InfoDivers] ps
	on e.idInfoDivers = ps.idInfoDivers
	and ps.Actif = 1

where 
e.Actif = 1
and e.IdAction = ?
and se.idSousEtape = ?

order by NumeroEtape</property>
