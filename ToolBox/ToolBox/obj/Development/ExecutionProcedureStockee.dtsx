        /// To open Help, press F1.
        /// </summary>
		public void Main()
		{
			// TODO: Add your code here

            string connectionString = "Data Source=" + Dts.Variables["$Package::NomInstance"].Value.ToString() + ";Initial Catalog=" + Dts.Variables["$Package::NomBDD"].Value.ToString() + ";Provider=SQLNCLI11.1;Integrated Security=SSPI;Auto Translate=False;";

            string [] parametreIn = Dts.Variables["$Package::ChaineDeParametres"].Value.ToString().Split(';');
                     
            
            
            // exécution de la procédure stockée
            using (System.Data.OleDb.OleDbConnection cnx = new System.Data.OleDb.OleDbConnection(connectionString))
            {

                OleDbCommand Storproc = new OleDbCommand();
                Storproc.Connection = cnx;
                Storproc.CommandType = CommandType.StoredProcedure;

                Storproc.CommandText = Dts.Variables["$Package::NomProcedureStockee"].Value.ToString();

                // Paramètres entrants
                int i = 0;
                foreach (string param in parametreIn)
                {
                    // le premier parametre de la chaine correspond au nom de fichier de la table InfoFichier
                    if (i == 0)
                        Dts.Variables["User::strNomFichier"].Value = param;
                    else
                    {
                        OleDbParameter p = new OleDbParameter("@P" + i.ToString(), OleDbType.VarWChar, Int32.MaxValue);
                        Storproc.Parameters.Add(p);
                        p.Direction = ParameterDirection.Input;
                        p.Value = param;
                    }
                    i++;
                }
                
                // Le paramètre sortants
                OleDbParameter pOut = new OleDbParameter("@Output", OleDbType.VarWChar, Int32.MaxValue);
                Storproc.Parameters.Add(pOut);
                pOut.Direction = ParameterDirection.Output;

                cnx.Open();
                Storproc.ExecuteNonQuery();
                Dts.Variables["User::strValeurRetour"].Value = pOut.Value.ToString();
            }


            Dts.TaskResult = (int)ScriptResults.Success;

