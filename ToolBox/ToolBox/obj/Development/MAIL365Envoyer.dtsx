                ExchangeService service = new ExchangeService(ExchangeVersion.Exchange2013);
                service.Timeout = (int)Dts.Variables["$Package::SmtpTimeout"].Value * 1000;
              //  service.Credentials = new OAuthCredentials(authResult.AccessToken);
                service.Credentials = new WebCredentials(Dts.Variables["$Package::SmtpLogin"].Value.ToString(), Dts.Variables["$Package::SmtpPwd"].Value.ToString());
                service.Url = new Uri(Dts.Variables["$Package::Server"].Value.ToString());

             //   service = ExchangeAuth.getExchangeService();

                EmailMessage message = new EmailMessage(service);

                message.From = Dts.Variables["$Package::From"].Value.ToString();
                message.Subject = Dts.Variables["$Package::Sujet"].Value.ToString();
                

                string[] sTo = Dts.Variables["$Package::To"].Value.ToString().Split(';');
                string[] sCc = Dts.Variables["$Package::Cc"].Value.ToString().Split(';');
                string[] sBcc = Dts.Variables["$Package::Bcc"].Value.ToString().Split(';');

                
                foreach(string s in sTo)
                {
                    if (s != "")
                        message.ToRecipients.Add(s);
                }
                foreach(string s in sCc)
                {
                    if (s != "")
                        message.CcRecipients.Add(s);
                }
                foreach(string s in sBcc)
                {
                    if (s != "")
                        message.BccRecipients.Add(s);
                }

                if (!Dts.Variables["$Package::DossierSource"].Value.ToString().Equals(""))
                {
                    string[] files = Directory.GetFiles(Dts.Variables["$Package::DossierSource"].Value.ToString(), Dts.Variables["$Package::Fichier"].Value.ToString() + "." + Dts.Variables["$Package::Extension"].Value.ToString());
                    foreach (string file in files)
                    {
                        message.Attachments.AddFileAttachment(file);
                    }
                }

                if ((bool)(Dts.Variables["$Package::AccuseReception"].Value))
                {
                    message.IsDeliveryReceiptRequested = true;
                    message.IsResponseRequested = true;
                }
                
                message.SendAndSaveCopy();             
                Dts.TaskResult = (int)ScriptResults.Success;
                
            }
            catch(Exception ex)
            {
                Dts.Variables["User::strResultMessage"].Value = ex.Message.ToString();
